<%= form_for :news, url: news_index_path do |f| %>
    <p>
      <%= f.label :judul %><br>
      <%= f.text_field :judul %>
    </p>

    <p>
      <%= f.label :penulis %><br>
      <%= f.text_field :penulis %>
    </p>

    <p>
      <%= f.label :isi %><br>
