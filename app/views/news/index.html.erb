<h1>Listing newss</h1>
<%= link_to 'New news', new_news_path %>
<table>
  <tr>
    <th>judul</th>
    <th>penulis</th>
    <th>isi</th>
    <th colspan="3"></th>
  </tr>

  <% @newss.each do |news| %>
      <tr>
        <td><%= news.judul %></td>
        <td><%= news.penulis %></td>
        <td><%= news.isi %></td>
        <td><%= link_to 'Show', news_path(news) %></td>
        <td><%= link_to 'Edit', edit_news_path(news) %></td>
        <td><%= link_to 'Destroy', news_path(news),
                        method: :delete,
                        data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
</table>